							"outputs": [
								{
									"referenceName": "ds_sqldwh_stg_table_tgt",
									"type": "DatasetReference",
									"parameters": {
										"schema_name": "dbo",
										"table_name": {
											"value": "@pipeline().parameters.stg_table",
											"type": "Expression"
										}
									}
								}
							]
						}
					]
				}
			},
			{
				"name": "Storedprocedure",
				"type": "SqlServerStoredProcedure",
				"dependsOn": [
					{
						"activity": "ForEach1",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"storedProcedureName": {
						"value": "@pipeline().parameters.proc_name",
						"type": "Expression"
					}
				},
				"linkedServiceName": {
					"referenceName": "ls_azure_sqldwh",
					"type": "LinkedServiceReference"
				}
			}
