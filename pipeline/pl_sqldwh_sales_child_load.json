										"source_folder": {
											"value": "@pipeline().parameters.source_folder",
											"type": "Expression"
										},
										"file_name": {
											"value": "@item().name",
											"type": "Expression"
										}
									}
								}
							],
							"outputs": [
								{
									"referenceName": "ds_sqldwh_azure_adlsgen2_file_binary_tgt",
									"type": "DatasetReference",
									"parameters": {
										"target_folder": {
											"value": "@pipeline().parameters.source_folder",
											"type": "Expression"
										},
										"target_file": {
											"value": "@item().name",
											"type": "Expression"
										}
